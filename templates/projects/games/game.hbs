{{> header}}
</br>
<div class="col-xs-9 col-sm-9 col-lg-9">
    <div class="panel panel-info">
        <div class="panel-heading">
            <h3 class="panel-title">{{name}}</h3>
            <div class="pull-right">
                <span>
                    <a href="#/projects/games">
                        <i class="fa fa-window-close"></i>
                    </a>
                </span>
            </div>
        </div>
        <div class="panel-body">
            <ul class="files-line">
                {{#each photos}}
                <li>
                    {{> photo}}
                </li>
                {{/each}}
                <li>
                    <span class="file">
                        <a target="_blank" href="{{link}}" class="fa fa-github fa-5x"></a>
                        <div>{{name}}</div>
                    </span>
                </li>
            </ul>
        </div>
    </div>
</div>
{{> footer}}